opencti:
  url: 'http://localhost:8080'
  token: 'ChangeMe'

connector:
  id: 'ChangeMe'
  type: 'INTERNAL_ENRICHMENT'
  name: 'DT-Lookup'
  scope: 'Autonomous-System,Domain-Name,Email-Addr,Ipv4-Addr'
  auto: false # Enable/disable auto-enrichment of observables
  confidence_level: 80 # From 0 (Unknown) to 100 (Fully trusted)
  log_level: 'info'

dtlookup:
  elasticsearch_url: 'ChangeMe'
  elasticsearch_index: 'ChangeMe'
  run_on_s: false
